<package name="DotNetLib" version="1.0" description="Sample package for using DotNet interop">

    <include file="Concerto/Core/Types.hpp" public="true"/>
    <include file="Concerto/DotNet/DotNetMethod.hpp" public="true"/>
    <include file="Concerto/DotNet/Assembly.hpp" public="false"/>

    <namespace name="cct">
      <namespace name="dotnet">
        <class name="DotNetLibAssembly" base="cct::dotnet::Assembly" autoGenerated="true">
            <method name="Hello" return="cct::Int32" base="cct::dotnet::DotNetMethod" customInvoke="true">
                <param name="arg" type="void*"/>
                <param name="size" type="cct::UInt32"/>
            </method>

            <method name="UpdateIntVectorWithoutCopy" return="cct::Int32" base="cct::dotnet::DotNetMethod" customInvoke="true">
                <param name="pointer" type="cct::Int32*"/>
                <param name="lenght" type="cct::Int32"/>
            </method>
        </class>
      </namespace>
    </namespace>
</package>